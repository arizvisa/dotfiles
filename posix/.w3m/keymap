# now we can start setting the keymaps to something "reasonable"
keymap C-f NEXT_PAGE
keymap C-b PREV_PAGE
 
keymap l MOVE_RIGHT
keymap h MOVE_LEFT
keymap j MOVE_DOWN
keymap k MOVE_UP
 
keymap J UP
keymap K DOWN
keymap zH SHIFT_LEFT
keymap zL SHIFT_RIGHT
keymap zh LEFT
keymap zl RIGHT
keymap UP UP
keymap DOWN DOWN
keymap C-LEFT SHIFT_LEFT
keymap C-RIGHT SHIFT_RIGHT
keymap LEFT LEFT
keymap RIGHT RIGHT
 
keymap \^ LINE_BEGIN
keymap 0 LINE_BEGIN
keymap $ LINE_END
 
keymap zz CENTER_V
keymap gg BEGIN
keymap G END
 
keymap w NEXT_WORD
keymap W PREV_WORD
keymap ] NEXT_LINK
keymap [ PREV_LINK
keymap C-n NEXT_LINK
keymap C-p PREV_LINK
keymap TAB NEXT_LINK

keymap gf MOVE_LIST_MENU

keymap gl NEXT_RIGHT
keymap gh NEXT_LEFT
keymap gj NEXT_DOWN
keymap gk NEXT_UP
 
keymap C-i REDO
keymap C-o UNDO
keymap zt CURSOR_TOP
keymap zb CURSOR_BOTTOM
 
keymap C-] GOTO_LINK
keymap C-j GOTO_LINK
keymap C-T TAB_LINK
 
keymap a SAVE_LINK
keymap d SAVE_LINK
keymap D SAVE_LINK
keymap gC-j SUBMIT
 
keymap u PEEK
keymap U PEEK_LINK
keymap gC-g INFO
keymap C-g LINE_INFO
 
keymap V LIST
keymap f LIST_MENU
 
keymap :o GOTO
keymap t TAB_GOTO
keymap :e LOAD
keymap :! PIPE_SHELL
keymap :%! PIPE_BUF
 
keymap B BACK
keymap :n NEXT
keymap :p PREV
keymap C-s SELECT_MENU
keymap :v VIEW
keymap :w SAVE
keymap C-r RELOAD
keymap C-l REDRAW
 
keymap C-t NEW_TAB
keymap C-w CLOSE_TAB
keymap gt NEXT_TAB
keymap gT PREV_TAB
keymap T TAB_MENU
keymap :t TAB_MENU
 
keymap :b VIEW_BOOKMARK
keymap :B ADD_BOOKMARK
 
keymap / ISEARCH
keymap ? ISEARCH_BACK
keymap n SEARCH_NEXT
keymap N SEARCH_PREV
 
# not sure if these actually work...
keymap m MARK
keymap gm NEXT_MARK
keymap gM PREV_MARK
 
keymap :H HELP
keymap :O OPTIONS
keymap :C COOKIE
keymap :h HISTORY
keymap :d DOWNLOAD_LIST
keymap :: COMMAND
 
keymap C-z SUSPEND
keymap :q QUIT
keymap :Q EXIT
